r\n\r\n        if (width >= textWidth) {\r\n            return; //space available\r\n        }\r\n        const diff = textWidth - width;\r\n        setOffsetWidth(`${diff}px`);\r\n\r\n    }, [ref.current ? ref.current.offsetWidth : 0]);\r\n\r\n    return (\r\n        <Wrapper ref={ref}>\r\n            { text &&\r\n                <CellText ref={textRef} color={color || currentTheme.dashboard.row.fontColor} \r\n                offset={dynamicText ? `${offsetWidth}` : undefined}>\r\n                    {text}\r\n                </CellText>\r\n            }\r\n            { image &&\r\n                <CellImg src={image.default} />\r\n            }\r\n        </Wrapper>\r\n    );\r\n}","D:\\Projects\\FE\\delivery-dashboard\\src\\components\\dashboard\\TitleCell.js",["373","374"],"import React, { useContext, useEffect, useState } from 'react';\r\nimport styled from \"styled-components\";\r\nimport { AppContext } from '../../contexts/AppContext';\r\n\r\nconst Wrapper = styled.div`\r\noverflow-x: hidden;\r\noverflow-y: hidden;\r\nwidth: -webkit-fill-available;\r\nwidth: -moz-available;\r\ntext-align: center;\r\n`;\r\n\r\nconst CellText = styled.p`\r\ntext-transform: uppercase;\r\nopacity: 0.6;\r\nfont-size: 10px;\r\nfont-weight: 500;\r\nfont-stretch: normal;\r\nfont-style: normal;\r\nline-height: normal;\r\nletter-spacing: normal;\r\ncolor: ${props => props.colorTheme.section.titleColor};\r\ndisplay: inline;\r\n`;\r\n\r\nconst CellImg = styled.img`\r\nsrc: \"${props => props.src}\";\r\nwidth: 80px;\r\nposition: fixed;\r\n`;\r\n\r\nexport function TitleCell({ text, image }) {\r\n\r\n    const { currentTheme } = useContext(AppContext);\r\n\r\n    return (\r\n        <Wrapper >\r\n            { text &&\r\n                <CellText colorTheme={currentTheme.dashboard}>\r\n                    {text}\r\n                </CellText>\r\n            }\r\n            { image &&\r\n                <CellImg src={image.default} />\r\n            }\r\n        </Wrapper>\r\n    );\r\n}","D:\\Projects\\FE\\delivery-dashboard\\src\\components\\dashboard\\CardRow.js",["375","376","377","378"],"import React, { useContext, useEffect, useState } from 'react';\r\nimport styled from \"styled-components\";\r\nimport { Cell } from './Cell';\r\nimport { Animated } from \"react-animated-css\";\r\nimport assets from '../../helpers/assets';\r\nimport { AppContext } from '../../contexts/AppContext';\r\n\r\nconst Wrapper = styled.div`\r\nbackground-color: ${props => props.colorTheme.row.backgroundColor};\r\ndisplay:  flex;\r\nflex-direction: row;\r\noverflow-x: hidden;\r\noverflow-y: hidden;\r\nwidth: 100%;\r\nmargin: 5px;\r\nmargin-left: 0px;\r\nborder-radius: 4px;\r\nheight: 55px;\r\nborder-width: 1px !important;\r\nborder: solid;\r\nborder-color: ${props => props.colorTheme.row.borderColor};\r\n`;\r\n\r\nconst orderStatusMap = {\r\n    \"ACCEPTED\": 'In Progress',\r\n    \"PREPARED\": 'In Progress',\r\n    \"READY_TO_DISPATCH\": 'Ready',\r\n    \"DISPATCHED\": 'Ready',\r\n}\r\n\r\nconst deliveryStatusMap = {\r\n    \"ON_ROUTE\": \"On Route\",\r\n    \"DRIVER_CHECKED_IN\": \"Checked In\"\r\n}\r\n\r\n\r\nexport function CardRow({ order }) {\r\n\r\n    const [visible, setVisible] = useState(true);\r\n    const [deliveryText, setDeliveryText] = useState('');\r\n    const [deliveryImage, setDeliveryImage] = useState('');\r\n\r\n    const { currentTheme, resources } = useContext(AppContext);\r\n\r\n    useEffect(() => {\r\n        console.log(\"order modified\");\r\n        setVisible(false);\r\n        setTimeout(() => {\r\n            setVisible(true);\r\n        }, 500);\r\n    }, [order]);\r\n\r\n    useEffect(() => {\r\n        setDeliveryImage(getDeliveryImage());\r\n    }, [order, currentTheme]);\r\n\r\n    const getOrderStatus = () => {\r\n        return orderStatusMap[order.status] || \"In Progress\";\r\n    }\r\n\r\n    const getDeliveryStatus = () => {\r\n        const pickedUp = order.statusHistory.find((history) => {\r\n            return history.status