ed.elapsedTime > waitingTime) {\r\n                updated = {\r\n                    ...updated,\r\n                    overTime: true,\r\n                };\r\n                overTimeCount += 1;\r\n            }\r\n            return updated;\r\n        }));\r\n        setOverTimeOrderCount(overTimeCount);\r\n    }\r\n\r\n    useEffect(() => {\r\n        setOffset(-1);\r\n        setOrderResponse([]);\r\n        setTheme();\r\n\r\n        if (mode === Modes.DISPATCHER) {\r\n            setSelectedTab(0);\r\n        }\r\n    }, [loggedIn, mode]);\r\n\r\n\r\n    const setTheme = () => {\r\n        if (!authData) {\r\n            return;\r\n        }\r\n        const localConfig = getFromStorage(authData.username);\r\n        if (localConfig) {\r\n            setCurrentTheme(Themes[localConfig.theme] || Themes['dark']);\r\n        }\r\n    }\r\n\r\n    useEffect(() => {\r\n        if (mode !== Modes.DRIVER_DASHBOARD) {\r\n            return;\r\n        }\r\n        if (pollCount * pollInterval >= pageSwitchInterval) {\r\n            // switch page\r\n            if (currentPage >= pages.length - 1) {\r\n                setCurrentPage(0);\r\n            } else {\r\n                setCurrentPage(currentPage => currentPage + 1);\r\n            }\r\n            setPollCount(0);\r\n        }\r\n\r\n    }, [pollCount]);\r\n\r\n    const doPoll = async () => {\r\n        if (!loggedIn) return;\r\n\r\n        if (mode === Modes.DRIVER_DASHBOARD) {\r\n            setPollCount(count => count + 1);\r\n        }\r\n\r\n        let response = undefined;\r\n        if (offset === -1) {\r\n            response = await fetchOrdersByLocation(kitchen, mode);\r\n            console.log(response.data);\r\n            setOrderResponse(sortOrders(response.data));\r\n        } else {\r\n            response = await fetchOrdersByOffset(kitchen, offset);\r\n            let modified = orderResponse;\r\n            for (let element of response.data) {\r\n                const existingOrder =